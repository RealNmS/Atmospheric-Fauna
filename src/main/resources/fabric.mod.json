{
  "schemaVersion": 1,
  "id": "atmospheric-fauna",
  "version": "${version}",
  "name": "Atmospheric Fauna",
  "description": "Minecraft mod that adds birds and other fauna making your world feel alive.",
  "authors": ["NmS"],
  "contact": {
    "homepage": "https://modrinth.com/project/atmospheric-fauna",
    "sources": "https://github.com/RealNmS/Atmospheric-Fauna",
    "issues": "https://github.com/RealNmS/Atmospheric-Fauna/issues"
  },
  "license": "CC0-1.0",
  "icon": "assets/atmospheric-fauna/icon.png",
  "environment": "*",
  "entrypoints": {
    "main": ["nms.atmosphericfauna.AtmosphericFauna"],
    "client": ["nms.atmosphericfauna.AtmosphericFaunaClient"],
    "modmenu": ["nms.atmosphericfauna.modmenu.ModMenuCompat"]
  },
  "mixins": ["atmospheric-fauna.mixins.json"],
  "depends": {
    "fabricloader": ">=0.18.2",
    "minecraft": "~1.21.11",
    "java": ">=21",
    "fabric-api": "*"
  },
  "recommends": {
    "modmenu": "*",
    "cloth-config": "*"
  },
  "custom": {
    "modmenu": {
      "links": {
        "modmenu.kofi": "https://ko-fi.com/onlynms"
      },
      "update_checker": true
    }
